{
  "name": "User Scripts Example",
  "version": "1.0",
  "manifest_version": 3,
  "minimum_chrome_version": "120",
  "description": "Demonstrates how to use the userScripts API.",
  "background": {
    "service_worker": "sw.js"
  },
  "permissions": ["storage", "userScripts"],
  "host_permissions": ["<all_urls>"],
  "action": {},
  "options_ui": {
    "page": "options.html",
    "open_in_tab": false
  }
}
